 type Query { materials(filter: MaterialFilter): [Material!]! }

input MaterialFilter {
    title: String
    requestFilter: RequestFilter
}

input RequestFilter {
    and: [RequestFilter!]
    or: [RequestFilter!]
    discountTypeFilter: DiscountTypeFilter
}

input DiscountTypeFilter {
    name: String
}

type Material {
    id: ID!
}

